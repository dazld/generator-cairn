{
    "extends":"airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
    },
    "rules": {
        "comma-dangle": 0,
        "func-names": 0,
        "prefer-rest-params": 0,
        "prefer-arrow-callback": 0,
        "space-before-function-paren": 0,
        "indent": [2, 4, {
            "SwitchCase": 1
        }],
        "react/jsx-indent-props": [2, 4],
        "react/jsx-closing-bracket-location": 0
    }
}
